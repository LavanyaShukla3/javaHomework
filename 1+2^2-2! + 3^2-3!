import java.util.Scanner;
class HelloWorld {
    public static float Factorial(int n){
        
        float factorial =1;
        for(float i = n; i>=1; i--){
            factorial = factorial*i;
            
        }
        return factorial;
    }
       

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        
        float q = 0;
        float fact= Factorial(n);
        
        for(float i = 1; i<= n; i++){
            q = (i*i)/ fact;
            q++;
            System.out.println(q);
        }
        
    }
    
}
